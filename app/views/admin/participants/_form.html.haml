= simple_form_for @participant, url: admin_participants_path, html: { class: 'form-horizontal' } do |f|
  = f.input :full_name, as: :string
  = f.input :school, as: :string
  = f.association :report
  = f.input :group, as: :string
  = f.input :phone, as: :string
  .form-actions
    = f.button :submit, class: 'btn-primary'
