    %small
      %a{ href: "http://github.com/ulmic/i_am_the_lider", target: "_blank" }
        = t('.this_project_on_the_Github')
    |
    %small
      %a{ href: "http://vk.com/ugosever", target: "_blank" }
        = t('.designer_page')
  .span2
    %a{ href: configus.apps.chrome_web_store_url, target: "_blank" }
      = image_tag "app_markets/chrome_web_store.png"
  .span5.mini-logos
    .logo.mini.vk
      %a{ href: "http://vk.com/ulmic", target: "_blank" }
    .logo.mini.facebook
      %a{ href: "https://www.facebook.com/molcenter", target: "_blank" }
    .logo.mini.twitter
      %a{ href: "https://twitter.com/ulmicru", target: "_blank" }
    .logo.mini.gplus
      %a{ href: "https://plus.google.com/109518417712111969312/", target: "_blank" }
    .logo.mini.youtube
